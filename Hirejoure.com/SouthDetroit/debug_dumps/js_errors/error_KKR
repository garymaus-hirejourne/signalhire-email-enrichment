        if (pageDataLayerObject !== null) {
            (function () {
                function getLocalTimestamp() {
                    var now = new Date();
                    var year = now.getFullYear();
                    var month = ("0" + (now.getMonth() + 1)).slice(-2);
                    var day = ("0" + now.getDate()).slice(-2);
                    var hour = ("0" + now.getHours()).slice(-2);
                    var min = ("0" + now.getMinutes()).slice(-2);
                    var sec = ("0" + now.getSeconds()).slice(-2);
                    return year + "-" + month + "-" + day + "T" + hour + ":" + min + ":" + sec;
                }

                function pushKkrDataLayerProperties() {
                    window.kkrDataLayer = window.kkrDataLayer || [];
                    kkrDataLayer.push({
                                event: "pageView",
                                page: {
                                    name: pageDataLayerObject.properties.sitePageName,
                                    site_section: pageDataLayerObject.properties.siteSection,
                                    site_subsection1: pageDataLayerObject.properties.siteSubsection1,
                                    site_subsection2: pageDataLayerObject.properties.siteSubsection2,
                                    site_subsection3: pageDataLayerObject.properties.siteSubsection3,
                                    URL: window.location.href,
                                    language: pageDataLayerObject.properties['xdm:language'],
                                    type: pageDataLayerObject.properties.pageType,
                                    siteName: pageDataLayerObject.properties.siteName,
                                    country: pageDataLayerObject.properties.country
                                },
                                user: {
                                    trafficType : pageDataLayerObject.properties.trafficType
                                }

                            });
                }

                function deferAPI() {
                    const url = "\/content\/kkr\/sites\/global\/en\/about\/our\u002Dpeople" +'/jcr:content.ipdetection.json';
                    let xhr = new XMLHttpRequest();
                    xhr.open("GET", url);
                    xhr.send();
                    xhr.onload = () => {
                        const data = JSON.parse(xhr.responseText);
                        if(data.trafficType) {
                            pageDataLayerObject.properties.trafficType = data.trafficType;
                            sessionStorage.setItem('trafficType',data.trafficType);
                            pushKkrDataLayerProperties();
                        } else {
                            pageDataLayerObject.properties.trafficType = 'external';
                            sessionStorage.setItem('trafficType','external');
                            pushKkrDataLayerProperties();
                        }
                    };
                }
                pageDataLayerObject["properties"]["localTimestamp"] = getLocalTimestamp();
                if(!sessionStorage.getItem('trafficType')) {
                    deferAPI();
                } else {
                    pageDataLayerObject.properties.trafficType = sessionStorage.getItem("trafficType");
                    pushKkrDataLayerProperties();
                }
                adobeDataLayer.push({
                    page: pageDataLayerObject,
                    event: 'cmp:show',
                    eventInfo: {
                        path: 'page.properties'
                    }
                });
            })();
        }
    </script>

    

