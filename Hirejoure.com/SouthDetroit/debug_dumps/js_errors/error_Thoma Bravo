  $(document).ready(function(){
    $( "#quicksearch" ).focus(function() {
      $('.team-disclaimer').hide();
    });
    $( "#quicksearch" ).blur(function() {
      if($('#quicksearch').val().length > 0){
        $('.team-disclaimer').hide();
      } else {
        $('.team-disclaimer').show();
      }
    });

  });

</script>
    

