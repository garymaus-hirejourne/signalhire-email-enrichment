  $(document).ready(function(){
    if (navigator.appVersion.indexOf("Mac")!=-1)
    {
      $('.footer-container-wrapper').addClass('footer-mac-adjust');
      $('body').addClass('ios');
      $('html').addClass('ios-html');
    } 



    var userAgent = navigator.userAgent || navigator.vendor || window.opera;
    if (/windows phone/i.test(userAgent)) { $('html').addClass('windows'); }
    else if (/android/i.test(userAgent)) { $('html').addClass('android'); }
    else if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) { $('html').addClass('ios'); } 

    if (navigator.userAgent.indexOf('Safari') != -1 &&
        navigator.userAgent.indexOf('Chrome') == -1) {
      document.body.className += " safari";
    }
  }); 


  $(document).ready(function(){
    $('.team-popup-section').each(function () {
      var id = $(this).attr('id');
      id = id.replace(/\W+/g, '-');
      $(this).attr('id', id);
    }); 

    $('.team-details-cm').each(function () {
      var data = $(this).attr('data-item-name');
      data = data.replace(/\W+/g, '-');
      $(this).attr('data-item-name', data);
    }); 

    // @TODO: remove
    setTimeout(function() { $('body').removeClass('black-screen'); }, 1000);  

    setTimeout(function() { 
      if(window.location.href.match('/?id')){
        $("body").addClass("black-screen close-position");
      }
    }, 1001);  
  });
  
