$("h2[data-role='leadeship_team']").empty().prepend('Leadership Team');
$("h2[data-role='operationteam']").empty().prepend('Operating Partners and Advisors');
$("h2[data-role='all_investment_staff']").empty().prepend('Investment Staff');
}, 100);
});
$(document).ready(function(){
$('.breadcrumb-section').on('click',function(e){
$(".left-col .filter-data-group").delay(100).slideToggle();
});
//     [INFO]: remove comment to start role dropdown filter
$('.button').on('click',function(e){
let x = $(this).find('a').html();
$('.roles-breadcrumb-div h5').empty().prepend(x);
});  
// quick search regex
var qsRegex;
var buttonFilter;
// init Isotope
$grid = $('.grid').isotope({
itemSelector: '.element-item',
layoutMode: 'fitRows',
filter: function() {
var $this = $(this);
var searchResult = qsRegex ? $this.text().match( qsRegex ) : true;
var buttonResult = buttonFilter ? $this.is( buttonFilter ) : true;
//         return searchResult && buttonResult; 
return searchResult;
},
});
let groupTitles = $('#container > .element-item').map(function() { return $(this).data('team'); }).get();
groupTitles = groupTitles.filter(function (value, index) { return groupTitles.indexOf(value) === index; });
let conainsElmItem = $('#container'), clnItems = conainsElmItem.find('.element-item');
if (groupTitles.length> 1)
groupTitles.forEach(function(group) {
$( "<h2 class=\"grp-title\" data-role=\"" + group + "\">" + group.split('_').join(' ') + "</h2>" ).insertBefore( $('[data-team="' + group + '"]').eq(0) );
});
if ( window.location.href.indexOf("team/all") >= 0 ) {
// all staff
//       let newArr = $('[data-team="all_staff"]').clone().sort(function(a, b) {
