$('.popup-close-button img').click(function(){
var newurl = window.location.href.split('?')[0];
window.history.pushState({path:newurl},'',newurl);
}); 
$(window).load(function() {
var url = window.location.toString();
if ( url !== "https://www.thomabravo.com/team" ){
var idShow = url.substring(url.lastIndexOf('=') +1);
var idToClass = '#' + idShow ;
console.log(idShow);
$(idToClass).addClass('popup-show').show();
// $('body').addClass('black-screen') @was before
}
});  
</script></div>

</div><!--end widget-span -->
</div><!--end row-->
</div><!--end row-wrapper -->

    </div><!--end footer -->
</div><!--end footer wrapper -->

    
